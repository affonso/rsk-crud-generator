{
    "name": "rsk/crud-generator",
    "description": "Laravel CRUD Generator with shadcn/ui React components for Laravel 12 Starter Kit",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RSK",
            "email": "dev@rsk.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rsk\\CrudGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rsk\\CrudGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rsk\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test:coverage": "vendor/bin/pest --coverage",
        "pint": "vendor/bin/pint",
        "pint:test": "vendor/bin/pint --test"
    }
}
